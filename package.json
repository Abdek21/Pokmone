{
  "name": "pokmone",
  "version": "0.1.0",
  "description": "projet bot discord",
  "main": "main.js",
  "repository": "https://github.com/Abdek21/Pokmone.git",
  "author": "Abdek21 <abdek.housssein@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^12.1.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-aqua": "^7.2.3"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off",
      "prefer-template": "off",
      "no-eval": "off"
    }
  }
}
